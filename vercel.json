{
  "version": 2,
  "routes": [
    {
      "src": "/auth/callback",
      "dest": "/index.html"
    },
    {
      "src": "/auth.*",
      "dest": "/index.html"
    },
    {
      "src": "/client.*",
      "dest": "/index.html"
    },
    {
      "src": "/admin.*",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
} 