{
  "rewrites": [
    { "source": "/client/:path*", "destination": "/" },
    { "source": "/auth/:path*", "destination": "/" },
    { "source": "/admin/:path*", "destination": "/" },
    { "source": "/(.*)", "destination": "/" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
} 